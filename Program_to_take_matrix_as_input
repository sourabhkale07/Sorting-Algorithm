#include<iostream>
using namespace std;

int main(){
    int m,n;
    
    cout<<"Enter the number of rows in matrix:"<<endl;
    cin>>m;
    cout<<"Enter the number of columns in matrix:"<<endl;
    cin>>n;
    
    int arr[m][n];
    
    for(int i =0;i<m;i++){
        for(int j =0;j<n;j++){
            cout<<"Enter the "<<i+1<<" row "<<j+1<<" column element "<<endl;
            cin>>arr[i][j];
        }
    }
    
    for(int i =0;i<m;i++){
        for(int j =0;j<n;j++){
           // cout<<"Enter the "<<i+1<<" row "<<j+1<<" column element "<<endl;
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}
